{
	"scripts": {
		"i": "npm install && npm run fix:dts",
		"fix:dts": "rm -rf node_modules/vuex/types/vue.d.ts && rm -rf node_modules/@types/echarts/index.d.ts",
		"rm": "npm run rm:flush",
		"rm:flush": "rm -rf node_modules && npm run rm:dists",
		"rm:dists": "rm -rf dist && run-p client:rm:dist server:rm:dist",
		"rc": "run-s client:rm:dist client:webpack:dev",
		"client:rm:dist": "rm -rf client/dist",
		"client:webpack:dev": "cross-env args=dev:watch webpack --config client.webpack.config",
		"rs": "run-s server:rm:dist server:dev",
		"rs2": "run-s server:rm:dist server:webpack:dev",
		"server:rm:dist": "rm -rf server/dist",
		"server:dev": "tsc -p server/server.tsconfig.json",
		"server:webpack:dev": "cross-env args=dev:watch webpack --config server.webpack.config",
		"server:forever:dev": "forever --killTree --watch --watchDirectory server/dist/build.js server/dist/build.js",
		"tscc": "clear && printf \"\\e[3J\" && echo 'Running TypeScript Compiler...\n' && tsc -p server/server.tsconfig.json --diagnostics || true",
		"tsc": "tsc"
	},
