{
	"scripts": {
		"i": "npm install || true && npm run fixdts",
		"fixdts": "rimraf node_modules/vuex/types/vue.d.ts && rimraf node_modules/@types/echarts/index.d.ts",
		"rm": "npm run server:kill && npm run rm:flush",
		"rm:flush": "rimraf node_modules && npm run rm:dists",
		"rm:dists": "rimraf dist && npm-run-all --parallel client:rm:dist server:rm:dist",
		"tsc": "npm run terminal:clear && echo 'Running TypeScript Compiler...\n' && tsc --diagnostics || true && rimraf dist",
		"rc": "npm run client:rm:dist && npm run client:webpack:dev:watch",
		"rcw": "npm run client:rm:dist && npm run client:webpack:dev:vendor",
		"client:rm:dist": "rimraf client/public/dist",
		"client:webpack:dev:watch": "cross-env args=dev:watch webpack --config client.webpack.config --watch",
		"client:webpack:dev:vendor": "cross-env args=dev:vendor webpack --config client.webpack.config",
		"client:webpack:watch": "cross-env args=dev:watch webpack --config client.webpack.config --watch",
		"client:webpack:build": "cross-env WEBPACK_ENV=PRODUCTION webpack --config client.webpack.config -p --progress --hide-modules --colors",
		"client:vendor:webpack:build": "webpack --config client.vendor.webpack.config",
		"rs": "npm run server:kill && npm run server:rm:dist && npm run server:webpack:dev",
		"k": "npm run server:kill",
		"server:kill": "forever stop acointrader-development || true && forever stop acointrader-production || true && npm run server:rm:logs",
		"server:killer": "forever stopall && npm run server:rm:logs",
		"server:rm:dist": "rimraf server/dist",
		"server:rm:logs": "rimraf ~/.forever/acointrader-development.log || true && rimraf ~/.forever/acointrader-production.log || true",
		"server:webpack:dev": "cross-env NODE_ENV=DEVELOPMENT webpack --config server.webpack.config --progress --hide-modules --colors --watch",
		"server:webpack:build": "cross-env NODE_ENV=PRODUCTION webpack --config server.webpack.config --progress --hide-modules --colors",
		"server:boot:development": "npm run server:kill && forever start -c 'node --expose-gc --max_old_space_size=512' forever-development.json && tail -F ~/.forever/acointrader-development.log",
		"server:boot:production": "npm run server:kill && forever start -c 'node --expose-gc --max_old_space_size=512' forever-production.json && tail -F ~/.forever/acointrader-production.log",
		"terminal:clear": "clear && printf \"\\e[3J\""
	},
